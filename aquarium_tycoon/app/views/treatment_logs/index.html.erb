<p id="notice"><%= notice %></p>

<h1>Treatment Logs</h1>
<br>
<%= link_to 'New Treatment Log', new_treatment_log_path %>
<br>
<table>
  <thead>
    <tr>
      <th>Treatment</th>
      <th>Time</th>
      <th>Medicine</th>
      <th>Quantity</th>
      <th>Employee</th>
      <th>Animal</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @treatment_logs.each do |treatment_log| %>
      <tr>
        <td><%= treatment_log.treatment_id %></td>
        <td><%= treatment_log.time %></td>
        <td><%= treatment_log.medicine %></td>
        <td><%= treatment_log.quantity %></td>
        <td><%= treatment_log.employee_id %></td>
        <td><%= treatment_log.animal_id %></td>
        <td><%= link_to 'Show', treatment_log %></td>
      </tr>
    <% end %>
  </tbody>
</table>




